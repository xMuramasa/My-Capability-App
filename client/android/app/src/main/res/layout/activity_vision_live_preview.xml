        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@id/preview_view"
        app:layout_constraintRight_toRightOf="@id/preview_view"
        app:layout_constraintTop_toTopOf="@id/preview_view"
        app:layout_constraintBottom_toBottomOf="@id/preview_view" />

    <ToggleButton
        android:id="@+id/facing_switch"
        android:layout_width="55dp"
        android:layout_height="58dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="8dp"
        android:background="@layout/toggle_style"
        android:checked="false"
        android:padding="12dp"
        android:textOff=""
        android:textOn=""
        app:layout_constraintBottom_toBottomOf="@id/preview_view"
        app:layout_constraintStart_toStartOf="parent" />

    <include
        android:id="@+id/timer_three"
        layout="@layout/timer_three_style"
        android:layout_width="102dp"
        android:layout_height="98dp"
        android:padding="12dp"
        app:layout_constraintLeft_toLeftOf="@id/preview_view"
        app:layout_constraintTop_toTopOf="@id/preview_view" />

    <include
        android:id="@+id/timer_five"
        layout="@layout/timer_five_style"
        android:layout_width="102dp"
        android:layout_height="98dp"
        app:layout_constraintLeft_toLeftOf="@id/preview_view"
        app:layout_constraintTop_toTopOf="@id/preview_view" />

    <include
        android:id="@+id/timer_ten"
        layout="@layout/timer_ten_style"
        android:layout_width="102dp"
        android:layout_height="98dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/preview_view" />

    <include
        android:id="@+id/timer_off"
        layout="@layout/timer_off_style"
        android:layout_width="102dp"
        android:layout_height="98dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/preview_view" />


</androidx.constraintlayout.widget.ConstraintLayout>
